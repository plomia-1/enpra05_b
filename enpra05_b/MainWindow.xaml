        <TextBox HorizontalAlignment="Left" Name="text1_textbox" Height="150" Margin="50,150,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="100"/>
        <TextBox HorizontalAlignment="Left" Name="text2_textbox" Height="150" Margin="180,150,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="100"/>
        <TextBlock HorizontalAlignment="Left" Name="output_textblock" Height="150" Margin="500,145,0,0"  TextWrapping="Wrap" VerticalAlignment="Top" Width="200"/>
        <Button Content="出力" FontSize="20" HorizontalAlignment="Left" Height="40" Margin="350,282,0,0" VerticalAlignment="Top" Width="98" Click="Button_Click"/>
        <TextBlock HorizontalAlignment="Left" FontSize="20" Height="30" Margin="50,80,0,0" Text="Text1" TextWrapping="Wrap" VerticalAlignment="Top" Width="60"/>
        <TextBlock HorizontalAlignment="Left" FontSize="20" Height="30" Margin="180,80,0,0" Text="Text2" TextWrapping="Wrap" VerticalAlignment="Top" Width="60"/>
        <TextBlock HorizontalAlignment="Left" FontSize="20" Height="30" Margin="500,80,0,0" Text="Text" TextWrapping="Wrap" VerticalAlignment="Top" Width="60"/>

    </Grid>
</Window>
